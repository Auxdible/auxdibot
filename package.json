{
  "name": "auxdibot",
  "version": "1.0.0",
  "description": "Express API and Discord.js backend for Auxdibot.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts . --quiet --fix",
    "prepare": "cd .. && husky install client/.husky"
  },
  "author": "Auxdible",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@types/emoji-regex": "^9.2.0",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "axios": "^1.3.4",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "emoji-regex": "^10.2.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "toad-scheduler": "^2.3.0",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "_moduleAliases": {
    "@": "dist/"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2"
  }
